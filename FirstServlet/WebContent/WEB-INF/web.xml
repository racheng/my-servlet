<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XML.Schema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	http://java.sun.com/xml/ns/javaee/web-app_3_O.xsd">
	<servlet>
		<servlet-name>
			HelloServlet
		</servlet-name>
			<servlet-class>cc.openhome.HelloServlet</servlet-class>
			<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/helloUser.view</url-pattern>
	</servlet-mapping>
</web-app>